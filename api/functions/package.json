{
  "name": "@rallii/functions",
  "description": "Rallii API Functions",
  "main": "build/src/main.dart.js",
  "scripts": {
    "build": "pub run build_runner build --output=build",
    "watch": "pub run build_runner watch --output=build",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "concurrently \"yarn watch\" \"yarn serve\"",
    "lint": "dartfmt src/ -w"
  },
  "dependencies": {
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
